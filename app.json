{
  "expo": {
    "name": "ShowSpot",
    "slug": "showspot",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/showspoticon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#2a2882"
    },
    "ios": {
      "bundleIdentifier": "com.showspot.app",
      "buildNumber": "13",
      "supportsTablet": false,
      "requireFullScreen": true,
      "jsEngine": "jsc",
      "infoPlist": {
        "UIDeviceFamily": [1],
        "NSLocationWhenInUseUsageDescription": "ShowSpot uses your location to discover live music venues and shows near you.",
        "NSCameraUsageDescription": "ShowSpot needs camera access to let you upload profile photos and venue images.",
        "NSPhotoLibraryUsageDescription": "ShowSpot needs access to your photos to let you upload profile and venue images.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#2a2882"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.showspot.app",
      "versionCode": 1,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6152b5a1-bc62-47ad-b84f-513c01389690"
      }
    }
  }
}
